{
  "name": "up9",
  "displayName": "UP9",
  "publisher": "UP9, INC",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:up9.openTestsBrowser"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "up9.openTestsBrowser",
        "title": "UP9: Open Tests Browser"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "cd react-app && npm run build && cd ../ && tsc -p ./ && cpy react-app/dist/index.html out/",
    "test-compile": "tsc -p ./ && cpy react-app/dist/index.html out/",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/tmp": "^0.2.2",
    "@types/vscode": "^1.62.0",
    "@types/vscode-webview": "^1.57.0",
    "axios": "^0.24.0",
    "cpy-cli": "^3.1.1",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tmp": "^0.2.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0",
    "webpack": "^5.63.0"
  }
}
